{
  "name": "backoffice-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "module:create": "nest g module",
    "resource:create": "nest g resource"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.19.0",
    "@aws-sdk/s3-request-presigner": "^3.20.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/event-emitter": "^1.3.0",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^9.0.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.938.0",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "jsonwebtoken": "^8.5.1",
    "nestjs-typeorm-paginate": "^4.0.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/multer": "^1.4.6",
    "@types/node": "^14.14.44",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "plugin": "^0.3.3",
    "prettier": "^2.3.0",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^1.1.9",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/__helpers__/setup.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "dist",
      ".eslintrc.js",
      "ormconfig.ts",
      "coverage",
      "node_modules",
      "<rootDir>/src/database/migrations",
      ".module.ts",
      "<rootDir>/src/lambda.ts",
      "<rootDir>/src/main.ts",
      ".entity.ts",
      ".mock.ts",
      ".spec.ts"
    ]
  }
}
